#formulas
grouping_totals <- function(df){
require("dplyr")
require("lazyeval")
out<-drones_removed %>% group_by_(df) %>% filter(A_TTComplete > 0) %>%
summarize(count = n())
return(out)
}
grouping_totals_incomplete <- function(df){
